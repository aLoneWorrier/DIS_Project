<!--Your HTML code here-->
<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="/docs/4.1/assets/img/favicons/favicon.ico">
    <title>@ViewData["Title"] - MoviesDB</title>

    <!-- Bootstrap core CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">

    <!-- Custom styles for this template -->
    <link rel="stylesheet" href="~/css/site.css" media="screen,projection">
    <link rel="stylesheet" href="~/css/site2.css">
    <link href="https://fonts.googleapis.com/css?family=Montserrat" rel="stylesheet">
</head>
<body>

    <header>
        <nav>
            <div class="nav-wrapper">
                 <a href="#" class="brand-logo show-on-medium-and-up" style="padding-left:1cm;">
                     <img enable-background="new 0 0 24 24" height="30" width="30" src="~/img/icon1.jpg" />
                     <label style="font-size:large;color:white;">MoviesDB</label>
                 </a>
                <a href="#" data-target="mobile-demo" class="sidenav-trigger"><i class="material-icons">Menu</i></a>
                <ul id="nav-mobile" class="right hide-on-med-and-down">
                    <li><a asp-controller="Home" asp-action="Index">Home</a></li>
                    <li><a asp-controller="Movie" asp-action="Create">Create</a></li>
                    <li><a asp-controller="Movie" asp-action="Search">Search</a></li>
                    <li><a asp-controller="Movie" asp-action="Model">Visualize</a></li>
                    <li><a asp-controller="Home" asp-action="AboutUs">AboutUs</a></li>
                </ul>
            </div>
        </nav>
        <ul class="sidenav" id="mobile-demo">
            <li><a asp-controller="Home" asp-action="Index">Home</a></li>
            <li><a asp-controller="Movie" asp-action="Create">Create</a></li>
            <li><a asp-controller="Movie" asp-action="Search">Search</a></li>
            <li><a asp-controller="Movie" asp-action="Model">Visualize</a></li>
            <li><a asp-controller="Home" asp-action="AboutUs">AboutUs</a></li>
        </ul>
    </header>

    <main>
        @RenderBody()
    </main>

    <footer class="page-footer">
        <div class="footer-copyright">
            <div class="container" style="text-align: center;">
                <p>
                    <i>© 2022 All rights Reserved</i>
                </p>
            </div>
        </div>
    </footer>

    <!--JavaScript at end of body for optimized loading-->
    <script type="text/javascript" src="~/js/jquery-3.6.0.min.js"></script>
    <script type="text/javascript" src="~/js/materialize.js"></script>
    <script type="text/javascript" src="~/js/site.js"></script>
    <script type="text/javascript" src="~/js/main.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.5.0/Chart.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    @RenderSection("chart", false)
    @RenderSection("carousel", false)
</body>
</html>